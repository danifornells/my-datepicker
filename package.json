{
  "name": "my-datepicker",
  "version": "1.0.0",
  "description": "A datepicker could fit into a multi-brand design system ",
  "scripts": {
    "build": "npm run build:tokens && npm run build:icons && npm run build:sb",
    "build:tokens": "rimraf build/tokens && node .tasks/build-tokens.js",
    "build:icons": "node .tasks/build-icons.js",
    "build:sb": "rimraf build/storybook && build-storybook -c .storybook -o build/storybook",
    "deploy": "now build/storybook --name=my-datepicker",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src/**/*.js src/**/*.jsx --fix && eslint .tasks/**/*.js --no-ignore --fix && eslint .storybook/**/*.js .storybook/**/*.js --no-ignore --fix",
    "lint:css": "stylelint 'src/**/*.scss' --fix",
    "start": "start-storybook",
    "test": "jest"
  },
  "author": "Daniel Fornells",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "autoprefixer": "^9.6.4",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "glob": "^7.1.4",
    "jest": "^24.9.0",
    "jsdom": "^15.2.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "react-dom": "^16.10.2",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-dictionary": "^2.8.2",
    "style-loader": "^1.0.0",
    "stylelint": "^11.1.1",
    "stylelint-config-recommended": "^3.0.0"
  },
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.10.2"
  },
  "jest": {
    "testRegex": ".*.unit\\.js$"
  }
}
